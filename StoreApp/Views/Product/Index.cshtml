@using StoreApp.Models

@model IEnumerable<Product>

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    @foreach(var p in Model)
    {
        <div>
            <h3>@p.ProductName</h3>
            <p>@p.Price</p>
        </div>
    }
</body>
</html>